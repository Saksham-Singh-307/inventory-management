def insertdi():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('billing')
    def savedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0 or len(be.get())==0 or len(ce.get())==0 or len(de.get())==0 or len(ee.get())==0 or len(fe.get())==0:
            messagebox.showerror("ERROR","PLS FILL THE DATA PROPERLY")
        xa=int(ae.get())
        xb=int(be.get())
        xc=int(ce.get())
        xf=int(fe.get())
        xe=int(ee.get())
        xd=int(de.get())
        sql="insert into dispatch values('%d','%d','%d','%d','%d','%d')"%(xa,xb,xc,xf,xd,xe)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('hi','data saved')
        db.close
        dc.append(xa)
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
    def close():
        t.destroy()
    a=Label(t,text='dispatchid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=Entry(t,width=10)
    ae.place(x=150,y=50)
    b=Label(t,text='orderid',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='custid',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    f=Label(t,text='productid',font='arial',fg='mediumblue')
    f.place(x=50,y=180)
    fe=Entry(t,width=5)
    fe.place(x=150,y=180)
    d=Label(t,text='qty',font='arial',fg='mediumblue')
    d.place(x=50,y=220)
    de=Entry(t,width=10)
    de.place(x=150,y=220)
    e=Label(t,text='billamount',font='arial',fg='mediumblue')
    e.place(x=50,y=270)
    ee=Entry(t,width=20)
    ee.place(x=150,y=270)
    btn=Button(t,text='save',fg='yellow',bg='maroon',command=savedata)
    btn.place(x=50,y=320)
    btn1=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn1.place(x=100,y=320)
    t.mainloop()
def removedatadi():
    dc.clear()
def filldatadi():
    db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
    cur=db.cursor()
    sql="select dispatchno from dispatch"
    cur.execute(sql)
    data=cur.fetchall()
    for i in data:
        dc.append(i)
def deletedatadi():
    filldatadi()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def deletedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="delete from dispatch where dispatchno=%d"%(xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('i','data deleted')
        removedatadi()
        ae.delete(0,100)
        
    def close():
        removedatadi()
        t.destroy()
    a=Label(t,text='dispatchno',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=dc
    ae.place(x=150,y=50)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='delete',fg='yellow',bg='maroon',command=deletedata)
    btn3.place(x=200,y=320)
    t.mainloop()

def finddi():
    filldatadi()
    global sc
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="select ordereid,cust,productid,qty,billamount from dispatch where dispatchno=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,str(data[0]))
        ce.insert(0,str(data[1]))
        de.insert(0,str(data[2]))
        ee.insert(0,str(data[3]))
        fe.insert(0,str(data[4]))
        db.close()
    def newdata():
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        ee.delete(0,100)
        fe.delete(0,100)
    def close():
        removedata()
        t.destroy()
    t.config(bg='red')
    a=Label(t,text='dispatch no.',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=dc
    ae.place(x=150,y=50)
    ab=Button(t,text='find',fg='yellow',bg='maroon',command=find)
    ab.place(x=300,y=50)
    b=Label(t,text='order id',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='customer id',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='product id',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='quantity',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='billamount',font='arial',fg='mediumblue')
    f.place(x=50,y=270)
    fe=Entry(t,width=10)
    fe.place(x=150,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn2=Button(t,text='new',fg='yellow',bg='maroon',command=newdata)
    btn2.place(x=150,y=320)
    t.mainloop()
def updatedatadi():
    filldatadi()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0:
            messagebox.showerror("ERROR","PLs select DISPATCH id ")
        xa=int(ae.get())
        sql="select ordereid,cust,productid,qty,billamount from dispatch where dispatchno=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,str(data[0]))
        ce.insert(0,str(data[1]))
        de.insert(0,str(data[2]))
        ee.insert(0,str(data[3]))
        fe.insert(0,str(data[4]))
        db.close()
    def update():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0:
            messagebox.showerror("ERROR","PLs select DISPATCH id ")
        xa=int(ae.get())
        xb=int(be.get())
        xc=int(ce.get())
        xd=int(de.get())
        xe=int(ee.get())
        xf=int(fe.get())
        sql="update dispatch set ordereid=%d,cust=%d,productid=%d,qty=%d,billamount=%d where dispatchno=%d"%(xb,xc,xd,xe,xf,xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('Hi','updated')
        ae.delete(0,100)
        be.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
        ce.delete(0,100)
        
                    
    def close():
        removedatadi()
        t.destroy()
    a=Label(t,text='dispatchno',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=dc
    ae.place(x=150,y=50)
    ab=Button(t,text='find',fg='yellow',bg='maroon',command=find)
    ab.place(x=350,y=50)
    b=Label(t,text='ordereid',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='cust',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='productid',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='quantity',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='billamount',font='arial',fg='mediumblue')
    f.place(x=50,y=270)
    fe=Entry(t,width=10)
    fe.place(x=150,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='update',fg='yellow',bg='maroon',command=update)
    btn3.place(x=200,y=320)
    t.mainloop()
