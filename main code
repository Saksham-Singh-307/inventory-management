import tkinter
from tkinter import *
from tkinter import ttk
from tkinter import messagebox
import pymysql
t=tkinter.Tk()
t.geometry('800x8000')
t.title('dashboard')
mydata=''
mydatacat=''
mydatapr=''
mydatasp=''
mydatacc=''
mydatass=''
mydatao=''
mydatadi=''
sc=[]
pc=[]
psc=[]
sp=[]
cc=[]
si=[]
oc=[]
dc=[]
def insert():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('dashboard storeinfo')
    def savedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0 or len(be.get())==0 or len(ce.get())==0 or len(de.get())==0 or len(ee.get())==0 or len(fe.get())==0:
            messagebox.showerror("ERROR","PLS FILL THE DATA PROPERLY")
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        xf=fe.get()
        xe=int(ee.get())
        xd=de.get()
        sql="insert into storeinfo values('%d','%s','%s','%s','%s','%d')"%(xa,xb,xc,xf,xd,xe)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('hi','data saved')
        db.close
        sc.append(xa)
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
    def close():
        t.destroy()
    a=Label(t,text='storeid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=Entry(t,width=10)
    ae.place(x=150,y=50)
    b=Label(t,text='name',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='address',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    f=Label(t,text='phone no',font='arial',fg='mediumblue')
    f.place(x=50,y=180)
    fe=Entry(t,width=5)
    fe.place(x=150,y=180)
    d=Label(t,text='email',font='arial',fg='mediumblue')
    d.place(x=50,y=220)
    de=Entry(t,width=10)
    de.place(x=150,y=220)
    e=Label(t,text='regno',font='arial',fg='mediumblue')
    e.place(x=50,y=270)
    ee=Entry(t,width=20)
    ee.place(x=150,y=270)
    btn=Button(t,text='save',command=savedata,fg='yellow',bg='maroon')
    btn.place(x=50,y=320)
    btn1=Button(t,text='close',command=close,fg='yellow',bg='maroon')
    btn1.place(x=100,y=320)
    t.mainloop()
def removedata():
    sc.clear()
def filldata():
    db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
    cur=db.cursor()
    sql="select storeid from storeinfo"
    cur.execute(sql)
    data=cur.fetchall()
    for i in data:
        sc.append(i)
            
    
        
    db.close()
def delete():
    removedata()
    filldata()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def deletedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        xs=str(xa)
        sql="delete from storeinfo where storeid=%d"%(xa)
        cur.execute(sql)
        db.commit
        messagebox.showinfo('i','data deleted')
        removedata()
        db.close()
        ae.delete(0,100)
        
    def close():
        removedata()
        t.destroy()
    t.config(bg='darkgreen')
    a=Label(t,text='storeid',font='arial',fg='mediumblue',bg='darkgreen')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=sc
    ae.place(x=150,y=50)
    btn=Button(t,text='close',fg='yellow',bg='mediumblue',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='delete',fg='yellow',bg='mediumblue',command=deletedata)
    btn3.place(x=200,y=320)
    t.mainloop()

def find():
    filldata()
    global sc
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="select name,address,phone,email,regno from storeinfo where storeid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        de.insert(0,data[2])
        ee.insert(0,data[3])
        fe.insert(0,str(data[4]))
        db.close()
    def newdata():
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        ee.delete(0,100)
        fe.delete(0,100)
    def close():
        removedata()
        t.destroy()
    a=Label(t,text='storeid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=sc
    ae.place(x=150,y=50)
    ab=Button(t,text='find',command=find,fg='yellow',bg='maroon')
    ab.place(x=300,y=50)
    b=Label(t,text='name',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='address',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='phone',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='email',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='registration no.',font='arial',fg='mediumblue')
    f.place(x=50,y=270)
    fe=Entry(t,width=10)
    fe.place(x=150,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn2=Button(t,text='new',fg='yellow',bg='maroon',command=newdata)
    btn2.place(x=150,y=320)
    t.mainloop()
def updatedata():
    filldata()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="select name,address,phone,email,regno from storeinfo where storeid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        de.insert(0,data[2])
        ee.insert(0,data[3])
        fe.insert(0,str(data[4]))
        db.close()
    def update():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        xd=de.get()
        xe=ee.get()
        xf=int(fe.get())
        sql="update storeinfo set name='%s',address='%s',phone='%s',email='%s',regno=%d where storeid=%d"%(xb,xc,xd,xe,xf,xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('Hi','updated')
        ae.delete(0,100)
        be.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
        ce.delete(0,100)
        
                    
    def close():
        removedata()
        t.destroy()
    a=Label(t,text='storeid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=sc
    ae.place(x=150,y=50)
    ab=Button(t,text='find',command=find,bg='maroon',fg='yellow')
    ab.place(x=400,y=50)
    b=Label(t,text='name',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='address',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='phoneno',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='email',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='registration no.',font='arial',fg='mediumblue')
    f.place(x=50,y=270)
    fe=Entry(t,width=10)
    fe.place(x=150,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='update',fg='yellow',bg='maroon',command=update)
    btn3.place(x=200,y=320)
    t.mainloop()
def showall():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    def filldata():
        bd=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=bd.cursor()
        sql="select storeid,name,address,phone,email,regno from storeinfo"
        cur.execute(sql)
        data=cur.fetchall()
        global mydata
        for res in data:
            mydata=mydata+str(res[0])+"\t"
            mydata=mydata+str(res[1])+"\t"
            mydata=mydata+(res[2])+"\t"
            mydata=mydata+(res[3])+"\t"
            mydata=mydata+(res[4])+"\t"
            mydata=mydata+str(res[5])+"\t"
            mydata=mydata+"\n"
        bd.close()
    a=Label(t,text='storeinfo Data Show')
    a.place(x=20,y=50)
    b=Text(t,width=250,height=300)
    b.place(x=20,y=50)
    filldata()
    b.insert(tkinter.END,mydata)
    t.mainloop()
def show():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    aa=[]
    ab=[]
    ac=[]
    ad=[]
    ae=[]
    af=[]
    i=0
    def filldata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        sql="select storeid,name,address,phone,email,regno from storeinfo"
        cur.execute(sql)
        data=cur.fetchall()
        global i
        for res in data:
            aa.append(res[0])
            ab.append(res[1])
            ac.append(res[2])
            ad.append(res[3])
            ae.append(res[4])
            af.append(res[5])
        db.close()
    def firstrecord():
        global i
        i=0
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
        
    def nextrecord():
        global i
        i=i+1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def prevrecord():
        global i
        i=i-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def lastrecord():
        global i
        i=len(aa)-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def close():
        t.destroy()


    a=Label(t,text='storeid',font='arial',fg='mediumblue')
    a.place(x=20,y=50)
    b=Label(t,width=30)
    b.place(x=500,y=50)
    c=Label(t,text='Name',font='arial',fg='mediumblue')
    c.place(x=20,y=150)
    d=Label(t,width=30)
    d.place(x=500,y=150)
    e=Label(t,text='address',font='arial',fg='mediumblue')
    e.place(x=20,y=200)
    f=Label(t,width=30)
    f.place(x=500,y=200)
    g=Label(t,text='phone',font='arial',fg='mediumblue')
    g.place(x=20,y=250)
    h=Label(t,width=30)
    h.place(x=500,y=250)
    bei=Label(t,text='email',font='arial',fg='mediumblue')
    bei.place(x=20,y=290)
    beie=Label(t,width=20)
    beie.place(x=500,y=290)
    x=Label(t,text='registration no',font='arial',fg='mediumblue')
    x.place(x=20,y=340)
    xee=Label(t,width=10)
    xee.place(x=500,y=340)
    k=Button(t,text='First',fg='yellow',bg='maroon',command=firstrecord)
    k.place(x=100,y=400)
    n=Button(t,text='Next',fg='yellow',bg='maroon',command=nextrecord)
    n.place(x=200,y=400)
    p=Button(t,text='Previous',fg='yellow',bg='maroon',command=prevrecord)
    p.place(x=300,y=400)
    r=Button(t,text='Last',fg='yellow',bg='maroon',command=lastrecord)
    r.place(x=400,y=400)
    rb=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    rb.place(x=500,y=400)
    filldata()
    t.mainloop()
def insertcat():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('billing')
    def savedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0 or len(be.get())==0 or len(ce.get())==0:
            messagebox.showerror("ERROR","PLS FILL THE DATA PROPERLY")
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        sql="insert into productcategory values('%d','%s','%s')"%(xa,xb,xc)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('hi','data saved')
        db.close
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
    def close():
        t.destroy()
    a=Label(t,text='catid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=Entry(t,width=10)
    ae.place(x=150,y=50)
    b=Label(t,text='categoryname',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='Discription',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    btn=Button(t,text='save',fg='yellow',bg='maroon',command=savedata)
    btn.place(x=50,y=180)
    btn1=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn1.place(x=150,y=180)
    t.mainloop()
def removedatacat():
    pc.clear()
def filldatacat():
    db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
    cur=db.cursor()
    sql="select catid from productcategory"
    cur.execute(sql)
    data=cur.fetchall()
    for i in data:
        pc.append(i)
def deletecat():
    filldatacat()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def deletedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="delete from productcategory where catid=%d"%(xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('i','data deleted')
        removedatacat()
        ae.delete(0,100)
        db.close()
        
    def close():
        removedatacat
        t.destroy()
    a=Label(t,text='category id',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=pc
    ae.place(x=150,y=50)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='delete',fg='yellow',bg='maroon',command=deletedata)
    btn3.place(x=200,y=320)
    t.mainloop()

