def showallsp():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    def filldata():
        bd=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=bd.cursor()
        sql="select supplyid,supplyname,supplyaddress,phoneno,emailid,registrationno from supplier"
        cur.execute(sql)
        data=cur.fetchall()
        global mydatasp
        for res in data:
            mydatasp=mydatasp+str(res[0])+"\t"
            mydatasp=mydatasp+(res[1])+"\t"
            mydatasp=mydatasp+(res[2])+"\t"
            mydatasp=mydatasp+(res[3])+"\t"
            mydatasp=mydatasp+(res[4])+"\t"
            mydatasp=mydatasp+str(res[5])+"\t"
            mydatsp=mydatasp+"\n"
        bd.close()
    a=Label(t,text='supplier Data Show')
    a.place(x=20,y=50)
    b=Text(t,width=250,height=300)
    b.place(x=20,y=50)
    filldata()
    b.insert(tkinter.END,mydatasp)
    t.mainloop()
def showsp():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    aa=[]
    ab=[]
    ac=[]
    ad=[]
    ae=[]
    af=[]
    i=0
    def filldata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        sql="select supplyid,supplyname,supplyaddress,phoneno,emailid,registrationno from supplier"
        cur.execute(sql)
        data=cur.fetchall()
        global i
        for res in data:
            aa.append(res[0])
            ab.append(res[1])
            ac.append(res[2])
            ad.append(res[3])
            ae.append(res[4])
            af.append(res[5])
        db.close()
    def firstrecord():
        global i
        i=0
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
        
    def nextrecord():
        global i
        i=i+1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def prevrecord():
        global i
        i=i-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def lastrecord():
        global i
        i=len(aa)-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def close():
        t.destroy()
    
    a=Label(t,text='supplyid ',font='arial',fg='mediumblue')
    a.place(x=20,y=50)
    b=Label(t,width=30)
    b.place(x=500,y=50)
    c=Label(t,text='supplyname',font='arial',fg='mediumblue')
    c.place(x=20,y=150)
    d=Label(t,width=30)
    d.place(x=500,y=150)
    e=Label(t,text='supplyaddress ',font='arial',fg='mediumblue')
    e.place(x=20,y=200)
    f=Label(t,width=30)
    f.place(x=500,y=200)
    g=Label(t,text='phoneno',font='arial',fg='mediumblue')
    g.place(x=20,y=250)
    h=Label(t,width=30)
    h.place(x=500,y=250)
    bei=Label(t,text='emailid',font='arial',fg='mediumblue')
    bei.place(x=20,y=290)
    beie=Label(t,width=20)
    beie.place(x=500,y=290)
    x=Label(t,text='registrationno',font='arial',fg='mediumblue')
    x.place(x=20,y=340)
    xee=Label(t,width=10)
    xee.place(x=500,y=340)
    k=Button(t,text='First',fg='yellow',bg='maroon',command=firstrecord)
    k.place(x=100,y=400)
    n=Button(t,text='Next',fg='yellow',bg='maroon',command=nextrecord)
    n.place(x=200,y=400)
    p=Button(t,text='Previous',fg='yellow',bg='maroon',command=prevrecord)
    p.place(x=300,y=400)
    r=Button(t,text='Last',fg='yellow',bg='maroon',command=lastrecord)
    r.place(x=400,y=400)
    btn1=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn1.place(x=450,y=400)
    filldata()
    t.mainloop()
def insertcc():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('customer')
    def savedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0 or len(be.get())==0 or len(ce.get())==0 or len(de.get())==0 or len(fe.get())==0:
            messagebox.showerror("ERROR","PLS FILL THE DATA PROPERLY")
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        xf=fe.get()
        xd=de.get()
        sql="insert into customer values('%d','%s','%s','%s','%s')"%(xa,xb,xc,xf,xd)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('hi','data saved')
        db.close
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
    def close():
        t.destroy()
    a=Label(t,text='Customerid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=Entry(t,width=10)
    ae.place(x=150,y=50)
    b=Label(t,text='Customername',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='Address',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    f=Label(t,text='phone',font='arial',fg='mediumblue')
    f.place(x=50,y=180)
    fe=Entry(t,width=5)
    fe.place(x=150,y=180)
    d=Label(t,text='email',font='arial',fg='mediumblue')
    d.place(x=50,y=220)
    de=Entry(t,width=10)
    de.place(x=150,y=220)
    btn=Button(t,text='save',fg='yellow',bg='maroon',command=savedata)
    btn.place(x=50,y=320)
    btn1=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn1.place(x=100,y=320)
    t.mainloop()
def removedatacc():
    cc.clear()
def filldatacc():
    db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
    cur=db.cursor()
    sql="select custid from customer"
    cur.execute(sql)
    data=cur.fetchall()
    for i in data:
        cc.append(i)
def deletedatacc():
    filldatacc()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def deletedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="delete from customer where custid=%d"%(xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('i','data deleted')
        cc.remove(xa)
        ae.delete(0,100)
        removedatacc()
        db.close()
        
    def close():
        removedatacc()
        t.destroy()
    a=Label(t,text='customer id',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=cc
    ae.place(x=150,y=50)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='delete',fg='yellow',bg='maroon',command=deletedata)
    btn3.place(x=200,y=320)
    t.mainloop()



