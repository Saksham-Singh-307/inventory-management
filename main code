def updatedatasp():
    filldatasp()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="select supplyname,supplyaddress,phoneno,emailid,registrationno from supplier where supplyid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        de.insert(0,data[2])
        ee.insert(0,data[3])
        fe.insert(0,str(data[4]))
        db.close()
    def update():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0:
            messagebox.showerror("ERROR","PLS SELECT THE SUPPLIER ID")
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        xd=de.get()
        xe=ee.get()
        xf=int(fe.get())
        sql="update supplier set supplyname='%s',supplyaddress='%s',phoneno='%s',emailid='%s',registrationno=%d where supplyid=%d"%(xb,xc,xd,xe,xf,xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('Hi','updated')
        ae.delete(0,100)
        be.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
        ce.delete(0,100)
        
                    
    def close():
        removedatasp()
        t.destroy()
    a=Label(t,text='supplier id',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=sp
    ae.place(x=150,y=50)
    ab=Button(t,text='find',fg='yellow',bg='maroon',command=find)
    ab.place(x=350,y=50)
    b=Label(t,text='supplier name',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='supplier address',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='phoneno',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='emailid',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='registration no',font='arial',fg='mediumblue')
