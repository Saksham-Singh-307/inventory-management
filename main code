            messagebox.showerror("ERROR","PLs select customer id ")
        xa=int(ae.get())
        xb=be.get()
        xc=ce.get()
        xd=de.get()
        xe=ee.get()
        sql="update customer set custname='%s',address='%s',phoneno='%s',emailid='%s' where custid=%d"%(xb,xc,xd,xe,xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('Hi','updated')
        ae.delete(0,100)
        be.delete(0,100)
        de.delete(0,100)
        ee.delete(0,100)
        ce.delete(0,100)
        
                    
    def close():
        removedatacc()
        t.destroy()
