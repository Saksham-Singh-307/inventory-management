            messagebox.showerror("ERROR","PLs select DISPATCH id ")
        xa=int(ae.get())
        sql="select custname,address,phoneno,emailid from customer where custid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        de.insert(0,data[2])
        ee.insert(0,data[3])
        db.close()
