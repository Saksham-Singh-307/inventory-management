def showallcat():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    def filldata():
        bd=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=bd.cursor()
        sql="select catid,catname,discription from productcategory"
        cur.execute(sql)
        data=cur.fetchall()
        global mydatacat
        for res in data:
            mydatacat=mydatacat+str(res[0])+"\t"
            mydatacat=mydatacat+str(res[1])+"\t"
            mydatacat=mydatacat+(res[2])+"\t"
            mydatacat=mydatacat+"\n"
        bd.close()
    a=Label(t,text='productcategory Data Show')
    a.place(x=20,y=50)
    b=Text(t,width=250,height=300)
    b.place(x=20,y=50)
    filldata()
    b.insert(tkinter.END,mydatacat)
    t.mainloop()
    
def showcat():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    aa=[]
    ab=[]
    ac=[]
    ad=[]
    ae=[]
    af=[]
    i=0
    def filldata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        sql="select catid,catname,discription from productcategory"
        cur.execute(sql)
        data=cur.fetchall()
        global i
        for res in data:
            aa.append(res[0])
            ab.append(res[1])
            ac.append(res[2])
        db.close()
    def firstrecord():
        global i
        i=0
        b.config(text='')
        d.config(text='')
        f.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        
    def nextrecord():
        global i
        i=i+1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
    def prevrecord():
        global i
        i=i-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
    def lastrecord():
        global i
        i=len(aa)-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
    def close():
        t.destroy()

    a=Label(t,text='category id',fg='mediumblue')
    a.place(x=20,y=50)
    b=Label(t,width=30)
    b.place(x=500,y=50)
    c=Label(t,text='category Name',fg='mediumblue')
    c.place(x=20,y=150)
    d=Label(t,width=30)
    d.place(x=500,y=150)
    e=Label(t,text='discription',fg='mediumblue')
    e.place(x=20,y=200)
    f=Label(t,width=30)
    f.place(x=500,y=200)
    k=Button(t,text='First',fg='yellow',bg='maroon',command=firstrecord)
    k.place(x=100,y=270)
    n=Button(t,text='Next',fg='yellow',bg='maroon',command=nextrecord)
    n.place(x=200,y=270)
    p=Button(t,text='Previous',fg='yellow',bg='maroon',command=prevrecord)
    p.place(x=300,y=270)
    r=Button(t,text='Last',fg='yellow',bg='maroon',command=lastrecord)
    r.place(x=400,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=500,y=270)
    filldata()
    t.mainloop()
def insertpr():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('billing')
    def savedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0 or len(be.get())==0 or len(ce.get())==0 or len(de.get())==0 or len(ee.get())==0 or len(fe.get())==0:
            messagebox.showerror("ERROR","PLS FILL THE DATA PROPERLY")
        xa=int(ae.get())
        xb=int(be.get())
        xc=ce.get()
        xf=int(fe.get())
        xe=int(ee.get())
        xd=de.get()
        sql="insert into products values('%d','%d','%s','%d','%s','%d')"%(xa,xb,xc,xf,xd,xe)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('hi','data saved')
        db.close
        ae.delete(0,100)
        be.delete(0,100)
        ce.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
    def close():
        t.destroy()
    a=Label(t,text='productid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=Entry(t,width=10)
    ae.place(x=150,y=50)
    b=Label(t,text='categoryid',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='Product name',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    f=Label(t,text='price',font='arial',fg='mediumblue')
    f.place(x=50,y=180)
    fe=Entry(t,width=5)
    fe.place(x=150,y=180)
