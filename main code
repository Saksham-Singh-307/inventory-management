def updatedatapr():
    filldatapr()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        if len(ae.get())==0:
            messagebox.showerror("ERROR","PLS SELECT THE PRODUCT ID")
        xa=int(ae.get())
        sql="select catid,pname,price,unit,qty from products where productid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        de.insert(0,data[2])
        ee.insert(0,data[3])
        fe.insert(0,str(data[4]))
        db.close()
    def update():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        xb=int(be.get())
        xc=ce.get()
        xd=int(de.get())
        xe=ee.get()
        xf=int(fe.get())
        sql="update products set catid=%d,pname='%s',price=%d,unit='%s',qty=%d where productid=%d"%(xb,xc,xd,xe,xf,xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('Hi','updated')
        ae.delete(0,100)
        be.delete(0,100)
        de.delete(0,100)
        fe.delete(0,100)
        ee.delete(0,100)
        ce.delete(0,100)
        
                    
    def close():
        removedatapr()
        t.destroy()
    a=Label(t,text='productid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=psc
    ae.place(x=150,y=50)
    ab=Button(t,text='find',fg='yellow',bg='maroon',command=find)
    ab.place(x=350,y=50)
    b=Label(t,text='category id',font='arial',fg='mediumblue')
    b.place(x=50,y=90)
    be=Entry(t,width=10)
    be.place(x=150,y=90)
    c=Label(t,text='product name',font='arial',fg='mediumblue')
    c.place(x=50,y=130)
    ce=Entry(t,width=10)
    ce.place(x=150,y=130)
    d=Label(t,text='price',font='arial',fg='mediumblue')
    d.place(x=50,y=180)
    de=Entry(t,width=10)
    de.place(x=150,y=180)
    e=Label(t,text='unit',font='arial',fg='mediumblue')
    e.place(x=50,y=220)
    ee=Entry(t,width=20)
    ee.place(x=150,y=220)
    f=Label(t,text='quantity',font='arial',fg='mediumblue')
    f.place(x=50,y=270)
    fe=Entry(t,width=10)
    fe.place(x=150,y=270)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='update',fg='yellow',bg='maroon',command=update)
    btn3.place(x=200,y=320)
    t.mainloop()
def showallpr():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    def filldata():
        bd=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=bd.cursor()
        sql="select productid,catid,pname,price,unit,qty from products"
        cur.execute(sql)
        data=cur.fetchall()
        global mydatapr
        for res in data:
            mydatapr=mydatapr+str(res[0])+"\t"
            mydatapr=mydatapr+str(res[1])+"\t"
            mydatapr=mydatapr+(res[2])+"\t"
            mydatapr=mydatapr+str(res[3])+"\t"
            mydatapr=mydatapr+(res[4])+"\t"
            mydatapr=mydatapr+str(res[5])+"\t"
            mydatpr=mydatapr+"\n"
        bd.close()
    a=Label(t,text='products Data Show')
    a.place(x=20,y=50)
    b=Text(t,width=250,height=300)
    b.place(x=20,y=50)
    filldata()
    b.insert(tkinter.END,mydatapr)
    t.mainloop()
def showpr():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    aa=[]
    ab=[]
    ac=[]
    ad=[]
    ae=[]
    af=[]
    i=0
    def filldata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        sql="select productid,catid,pname,price,unit,qty from products"
        cur.execute(sql)
        data=cur.fetchall()
        global i
        for res in data:
            aa.append(res[0])
            ab.append(res[1])
            ac.append(res[2])
            ad.append(res[3])
            ae.append(res[4])
            af.append(res[5])
        db.close()
    def firstrecord():
        global i
        i=0
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
        
    def nextrecord():
        global i
        i=i+1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def prevrecord():
        global i
        i=i-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def lastrecord():
        global i
        i=len(aa)-1
        b.config(text='')
        d.config(text='')
        f.config(text='')
        h.config(text='')
        beie.config(text='')
        xee.config(text='')
        
        b.config(text=aa[i])
        d.config(text=ab[i])
        f.config(text=ac[i])
        h.config(text=ad[i])
        beie.config(text=ae[i])
        xee.config(text=af[i])
    def close():
        t.destroy()
    a=Label(t,text='productid',font='arial',fg='mediumblue')
    a.place(x=20,y=50)
    b=Label(t,width=30)
    b.place(x=500,y=50)
    c=Label(t,text='category id',font='arial',fg='mediumblue')
    c.place(x=20,y=150)
    d=Label(t,width=30)
    d.place(x=500,y=150)
    e=Label(t,text='product name',font='arial',fg='mediumblue')
    e.place(x=20,y=200)
    f=Label(t,width=30)
    f.place(x=500,y=200)
    g=Label(t,text='price',font='arial',fg='mediumblue')
    g.place(x=20,y=250)
    h=Label(t,width=30)
    h.place(x=500,y=250)
    bei=Label(t,text='unit',font='arial',fg='mediumblue')
    bei.place(x=20,y=290)
    beie=Label(t,width=20)
    beie.place(x=500,y=290)
    x=Label(t,text='qty',font='arial',fg='mediumblue')
    x.place(x=20,y=340)
    xee=Label(t,width=10)
    xee.place(x=500,y=340)
    k=Button(t,text='First',fg='yellow',bg='maroon',command=firstrecord)
    k.place(x=100,y=400)
    n=Button(t,text='Next',fg='yellow',bg='maroon',command=nextrecord)
    n.place(x=200,y=400)
    p=Button(t,text='Previous',fg='yellow',bg='maroon',command=prevrecord)
    p.place(x=300,y=400)
    r=Button(t,text='Last',fg='yellow',bg='maroon',command=lastrecord)
    r.place(x=400,y=400)
    btn1=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn1.place(x=450,y=400)
    filldata()
    t.mainloop()
