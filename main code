def showallo():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    def filldata():
        bd=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=bd.cursor()
        sql="select orderid,custid,dateoforder,catid,productid,qty from orders"
        cur.execute(sql)
        data=cur.fetchall()
        global mydatao
        for res in data:
            mydatao=mydatao+str(res[0])+"\t"
            mydatao=mydatao+str(res[1])+"\t"
            mydatao=mydatao+(res[2])+"\t"
            mydatao=mydatao+str(res[3])+"\t"
            mydatao=mydatao+str(res[4])+"\t"
            mydatao=mydatao+str(res[5])+"\t"
            mydato=mydatao+"\n"
        bd.close()
    a=Label(t,text='orders Data Show')
    a.place(x=20,y=50)
    b=Text(t,width=250,height=300)
    b.place(x=20,y=50)
    filldata()
    b.insert(tkinter.END,mydatao)
    t.mainloop()
def showo():
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('Data Show')
    aa=[]
    ab=[]
    ac=[]
    ad=[]
    ae=[]
    af=[]
    i=0
    def filldata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        sql="select orderid,custid,dateoforder,catid,productid,qty from orders"
        cur.execute(sql)
        data=cur.fetchall()
        global i
        for res in data:
            aa.append(res[0])
            ab.append(res[1])
            ac.append(res[2])
            ad.append(res[3])
            ae.append(res[4])
            af.append(res[5])
        db.close()
