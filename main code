        for res in data:
            mydatapr=mydatacc+str(res[0])+"\t"
            mydatacc=mydatacc+(res[1])+"\t"
            mydatacc=mydatacc+(res[2])+"\t"
            mydatacc=mydatacc+(res[3])+"\t"
            mydatacc=mydatacc+(res[4])+"\t"
            mydatcc=mydatacc+"\n"
        bd.close()
