def removedatapr():
    psc.clear()
def filldatapr():
   db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
   cur=db.cursor()
   sql="select productid from products"
   cur.execute(sql)
   data=cur.fetchall()
   for i in data:
       psc.append(i)
       db.close()
def deletepr():
    filldatapr()
    t=tkinter.Tk()
    t.geometry('800x800')
    t.title('p1')
    def deletedata():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="delete from products where productid=%d"%(xa)
        cur.execute(sql)
        db.commit()
        messagebox.showinfo('i','data deleted')
        removedatapr()
        ae.delete(0,100)
        db.close()
        
    def close():
        removedatapr()
        t.destroy()
    a=Label(t,text='productid',font='arial',fg='mediumblue')
    a.place(x=50,y=50)
    ae=ttk.Combobox(t)
    ae['values']=psc
    ae.place(x=150,y=50)
    btn=Button(t,text='close',fg='yellow',bg='maroon',command=close)
    btn.place(x=50,y=320)
    btn3=Button(t,text='delete',fg='yellow',bg='maroon',command=deletedata)
    btn3.place(x=200,y=320)
    t.mainloop()

