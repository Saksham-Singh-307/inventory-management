    t.title('p1')
    def find():
        db=pymysql.connect(host='localhost',user='root',password='root',database='testdb')
        cur=db.cursor()
        xa=int(ae.get())
        sql="select catname,discription from productcategory where catid=%d"%(xa)
        cur.execute(sql)
        data=cur.fetchone()
        be.insert(0,data[0])
        ce.insert(0,data[1])
        db.close()
    def newdata():
        ae.delete(0,100)
        be.delete(0,100)
